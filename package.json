{
  "name": "Algo.js",
  "private": true,
  "version": "0.0.0",
  "description": "Implementation of some algorithms using JavaScript",
  "main": "./test/q.js",
  "dependencies": {},
  "devDependencies": {
    "grunt": "~0.4.2",
    "grunt-contrib-jshint": "~0.11.2",
    "grunt-contrib-nodeunit": "~0.4.1",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-es6-transpiler": "^1.0.2",
    "grunt-mocha-cov": "^0.4.0",
    "should": "^7.0.2"
  },
  "scripts": {    
    "test": "tar xjkvf test/graph/scc.tar -C test/graph/ --overwrite && grunt",
    "start": "wget -nc -P test/graph/ https://github.com/scotv/algo-js/releases/download/2.7182818284/scc99.7z && 7z e test/graph/scc99.7z -otest/graph/ -aoa"
  },
  "config": {
    "blanket": {
      "pattern": "//^((?!/node_modules/)(?!/test/).)*$/",
      "data-cover-never": ["test.js", "deploy/test.js"],
      "data-cover-flags": {
        "branchTracking": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/scotv/algo-js.git"
  },
  "keywords": [
    "algorithm",
    "JavaScript"
  ],
  "author": "scott",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/scotv/algo-js/issues"
  },
  "homepage": "https://github.com/scotv/algo-js/wiki"
}
